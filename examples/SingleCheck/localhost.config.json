    ,"ZeroAutoGrowthFiles":{
        "Check": true
        ,"Whitelist": [
            "AdventureWorks2016CTP3_mod"
            ,"templog"
        ]
    }
    ,"ShouldCheckForAutoGrowthRisks": true
    ,"CheckDuplicateIndexes": {
        "Check": true,
        "ExcludeDatabase": [ "msdb", "master", "tempdb", "model" ]
